{
  "name": "corporate-offsite-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run prerender && npm run sitemap",
    "build:seo": "npm run build && node scripts/prerender.js",
    "build:seo-deploy": "npm run build && npm run prerender && node scripts/deploy-seo.js",
    "build:full": "npm run sitemap && npm run build:seo-deploy",
    "build:vike": "vike build",
    "prerender": "vike prerender",
    "deploy-seo": "node scripts/deploy-seo.js",
    "sitemap": "node scripts/generate-sitemap.js",
    "analyze": "vite build && open dist/stats.html",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.15.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-sitemap": "^0.8.2",
    "vite-ssg": "^28.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "rollup-plugin-visualizer": "^6.0.3",
    "sitemap": "^8.0.0",
    "tailwindcss": "^3.3.3",
    "vike": "^0.4.236",
    "vite": "^4.4.5"
  }
}
